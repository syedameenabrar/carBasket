{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "carBasket": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/carBasket",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/style.css",
              "src/assets/vendor/nice-select/nice-select.css",

              "src/assets/css/navbar.css",

              "src/assets/css/custom-animation.css",
              "src/assets/css/framework.css",
              "src/assets/css/responsive.css",
              "src/assets/fonts/recoleta/stylesheet.css",

              "src/assets/fonts/gordita/stylesheet.css",

              "src/assets/fonts/eustache/stylesheet.css",
              "src/assets/fonts/noteworthy/style.css",
              "src/assets/fonts/bootstrap-icons/font-css.css",

              "src/assets/fonts/font-awesome/css/all.min.css",

              "src/assets/vendor/bootstrap/css/bootstrap.min.css",
              "src/assets/vendor/bootstrap/css/dist/css/bootstrap.css",
              "src/assets/vendor/bootstrap/css/bootstrap.min.css",

              "src/assets/vendor/fancybox/dist/jquery.fancybox.min.css",
             



              "src/assets/vendor/fancybox/dist/jquery.fancybox.min.css",
              "src/assets/vendor/slick/slick.css",
              "src/assets/vendor/nice-select/nice-select.css",
              "src/assets/vendor/wow/animate.css",
              "src/assets/vendor/slick/slick.css",
              "src/assets/vendor/wow/animate.css"


            ],
            "scripts": [
              "src/assets/vendor/jquery.min.js",
              "src/assets/vendor/nice-select/jquery.nice-select.min.js",
              "src/assets/vendor/fancybox/dist/jquery.fancybox.min.js",


              "src/assets/js/theme.js",
              "src/assets/vendor/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/vendor/js/index.umd.js",
              "src/assets/vendor/js/src/alert.js",
              "src/assets/vendor/js/src/base-component.js",
              "src/assets/vendor/js/src/button.js",
              "src/assets/vendor/js/src/carousel.js",
              "src/assets/vendor/js/src/collapse.js",
              "src/assets/vendor/js/src/dropdown.js",
              "src/assets/vendor/js/src/modal.js",
              "src/assets/vendor/js/src/offcanvas.js",
              "src/assets/vendor/js/src/popover.js",
              "src/assets/vendor/js/src/scrollspy.js",
              "src/assets/vendor/js/src/tab.js",
              "src/assets/vendor/js/src/toast.js",
              "src/assets/vendor/js/src/toast.js",
              "src/assets/vendor/js/src/tooltip.js",
              "src/assets/vendor/slick/slick.min.js",
              "src/assets/vendor/wow/wow.min.js",
              "src/assets/vendor/jquery.counterup.min.js",
              "src/assets/vendor/jquery.lazy.min.js",
              "src/assets/vendor/jquery.waypoints.min.js",
              "src/assets/vendor/validator.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "carBasket:build:production"
            },
            "development": {
              "browserTarget": "carBasket:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "carBasket:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "carBasket"
}
